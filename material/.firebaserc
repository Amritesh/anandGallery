{
  "projects": {
    "default": "anandmuseum",
    "master": "anandmuseum",
    "prod": "anandmuseum"
  },
  "ci": {
    "createConfig": {
      "master": {
        "env": "staging",
        "firebase": {
          "apiKey": "${STAGE_FIREBASE_API_KEY}",
          "authDomain": "anandmuseum.firebaseapp.com",
          "databaseURL": "https://anandmuseum.firebaseio.com",
          "projectId": "anandmuseum",
          "storageBucket": "anandmuseum.appspot.com"
        }
      },
      "prod": {
        "env": "production",
        "firebase": {
          "apiKey": "${PROD_FIREBASE_API_KEY}",
          "authDomain": "anandmuseum.firebaseapp.com",
          "databaseURL": "https://anandmuseum.firebaseio.com",
          "projectId": "anandmuseum",
          "storageBucket": "anandmuseum.appspot.com"
        }
      }
    }
  }
}
